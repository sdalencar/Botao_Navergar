<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TableRow
        android:layout_width="@dimen/width_04"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/modelo_quatro_primeiro"
            android:layout_width="@dimen/width_025"
            android:textColor="@color/branco"
            android:textSize="@dimen/texto20" />

        <TextView
            android:id="@+id/modelo_quatro_segundo"
            android:layout_width="@dimen/width_015"
            android:textColor="@color/branco"
            android:textSize="@dimen/texto20" />
    </TableRow>

    <TableRow
        android:layout_width="@dimen/width_04"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/modelo_quatro_terceiro"
            android:layout_width="@dimen/width_02"
            android:textColor="@color/branco"
            android:textSize="@dimen/texto20" />

        <TextView
            android:id="@+id/modelo_quatro_quarto"
            android:layout_width="@dimen/width_02"
            android:textColor="@color/branco"
            android:textSize="@dimen/texto20" />

    </TableRow>

    <TextView
        android:layout_width="@dimen/width_04"
        android:layout_height="wrap_content"
        android:background="@color/brancoOpaco" />


</androidx.appcompat.widget.LinearLayoutCompat>